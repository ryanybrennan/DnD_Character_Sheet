{
  "expo": {
    "name": "DnD_Character_Sheet",
    "slug": "DnD_Character_Sheet",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Capstone/side project request by my dungeons and dragons players to create a mobile character sheet.",
    "githubUrl": "https://github.com/ryanybrennan/DnD_Character_Sheet"
  }
}